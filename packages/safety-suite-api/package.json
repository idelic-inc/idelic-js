{
  "name": "@idelic/safety-suite-api",
  "version": "47.0.0",
  "description": "API helper library for Safety Suite",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "start": "tsc --watch",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "clean": "rm -rf lib",
