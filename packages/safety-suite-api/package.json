  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/idelic-inc/safety-suite-api.git"
  },
  "author": "Idelic",
  "license": "MIT",
  "dependencies": {
    "@idelic/safety-net": "^2.1.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "source-map-support": "^0.5.13"
  },
  "peerDependencies": {
    "immutable": "4.x"
  },
  "optionalDependencies": {
    "react": "^16.10.2"
  }
}
