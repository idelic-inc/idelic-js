{
  "name": "@idelic-js/safety-suite-api",
  "version": "8.0.0",
  "description": "API helper library for Safety Suite",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "start": "tsc --watch",
    "build": "tsc",
    "clean": "rm -r lib/*",
    "prepare": "yarn clean && yarn build",
    "prepublishOnly": "yarn test",
    "preversion": "yarn test",
    "version": "git add -A src scripts",
    "postversion": "git push && git push --tags",
    "generateTypes": "tsc ./scripts/generateModelTypes/index.ts --lib es7,dom --esModuleInterop --outDir ./bin/generateModelTypes && node ./bin/generateModelTypes/index.js \"$SCHEMA_PATH\" src/api/models/modelTypes.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/idelic-inc/safety-suite-api.git"
  },
  "author": "Idelic",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^13.9.3",
    "glob": "^7.1.6",
    "immutable": "^4.0.0-rc.12",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "idelic-safety-net": "^1.3.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "source-map-support": "^0.5.13"
  },
  "peerDependencies": {
    "immutable": "4.x"
  }
}
